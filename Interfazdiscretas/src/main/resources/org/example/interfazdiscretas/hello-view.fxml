<VBox alignment="CENTER" prefHeight="447.0" prefWidth="360.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.interfazdiscretas.HelloController">
